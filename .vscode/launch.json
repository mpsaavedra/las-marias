            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build_profile",
            "program": "${workspaceFolder}/src/Profile/API/bin/Debug/net6.0/LasMarias.Profile.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/Profile/API",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
